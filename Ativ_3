CREATE TABLE aluno (codigo int, nome text, telefone text, cidade text);
CREATE TABLE funcionarios (codigo int, nome text, endereco text, telefone text, cidade text, estado text,
cep text, rg text, cpf text, salario number);
ALTER TABLE aluno RENAME TO alunos;
